       \u003Cth\u003E\u003Cdiv style=\"color: #700000;\"\u003EValue 2:\u003C\/div\u003E\u003C\/th\u003E                \u003Cth\u003E\u003Cdiv style=\"color: #700000;\"\u003ETotal:\u003C\/div\u003E\u003C\/th\u003E            \u003C\/tr\u003E\u003Ctr\u003E                \u003Ctd\u003E\u003Cinput id=\"number1\" type=\"text\" \/\u003E\u003C\/td\u003E                \u003Ctd\u003E\u003Cinput id=\"number2\" type=\"text\" \/\u003E\u003C\/td\u003E                \u003Ctd\u003E\u003Cinput id=\"total\" type=\"text\" \/\u003E\u003C\/td\u003E            \u003C\/tr\u003E\u003C\/tbody\u003E    \u003C\/table\u003E\u003Cbr \/\u003E\u003Cscript type=\"text\/javascript\"\u003E    function add(){         document.getElementById(\"total\").value = parseFloat(document.getElementById(\"number1\").value) + parseFloat(document.getElementById(\"number2\").value);     }     function multiply(){      document.getElementById(\"total\").value = parseFloat(document.getElementById(\"number1\").value) * parseFloat(document.getElementById(\"number2\").value);  }  function minus(){     document.getElementById(\"total\").value = parseFloat(document.getElementById(\"number1\").value) - parseFloat(document.getElementById(\"number2\").value); } function divide(){     document.getElementById(\"total\").value = parseFloat(document.getElementById(\"number1\").value) \/ parseFloat(document.getElementById(\"number2\").value); } \u003C\/script\u003E \u003Cinput onclick=\"add()\" type=\"button\" value=\"Add\" \/\u003E\u0026nbsp;\u0026nbsp;\u0026nbsp;\u003Cinput onclick=\"minus()\" type=\"button\" value=\"minus\" \/\u003E\u0026nbsp;\u0026nbsp;\u0026nbsp;\u003Cinput onclick=\"multiply()\" type=\"button\" value=\"multiply\" \/\u003E\u0026nbsp;\u0026nbsp;\u0026nbsp;\u003Cinput onclick=\"divide()\" type=\"button\" value=\"divide\" \/\u003E"},"link":[{"rel":"replies","type":"application/atom+xml","href":"http:\/\/seleniumautomationhelper.blogspot.com\/feeds\/245466661739673309\/comments\/default","title":"Post Com